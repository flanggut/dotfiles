def printProgress (iteration, total, length = 80):
percent = int(100 * (iteration / float(total)))
filledLength = int(length * iteration // total)
bar = 'â–ˆ' * filledLength + '-' * (length - filledLength)
print(f"Progress: |{bar}| {percent}%", end = '\r')
if iteration == total:
print()
